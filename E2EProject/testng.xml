<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">

	<listeners>
		<listener class-name="Academy.E2EProject.Listeners" />
	</listeners>

	<test name="Validating navigation bar">
		<classes>
			<class name="Academy.E2EProject.validateNavigationBar" />
		</classes>
	</test>
	<test name="Validating title">
		<classes>
			<class name="Academy.E2EProject.validateTitle" />
		</classes>
	</test>
	<test name="Validating user login">
		<classes>
			<class name="Academy.E2EProject.validateUsers" />
		</classes>
	</test>
</suite>
